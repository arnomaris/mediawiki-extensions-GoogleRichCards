{
	"name": "GoogleRichCards",
	"version": "0.2",
	"author": [
		"Igor Shishkin",
		"Drolerina"
	],
	"url": "https://github.com/teran/mediawiki-GoogleRichCards",
	"description": "Google Rich Cards extension",
	"license-name": "GPL",
	"type": "parserhook",
	"MessagesDirs": {},
	"requires": {
		"MediaWiki": ">= 1.39.0"
	},
	"AutoLoadNamespaces": {
		"MediaWiki\\Extension\\GoogleRichCards\\": "includes/"
	},
	"config": {
		"wgGoogleRichCardsAnnotateArticles": {
			"value": true,
			"description": "Add support for Google Rich Cards to articles",
			"public": true
		},
		"wgGoogleRichCardsAnnotateEvents": {
			"value": false,
			"description": "Add support for Google Rich Cards to events",
			"public": true
		},
		"wgGoogleRichCardsAnnotateWebSite": {
			"value": false,
			"description": "Add support for Google Rich Cards to website",
			"public": true
		}
	},
	"Hooks": {
		"BeforePageDisplay": "MediaWiki\\Extension\\GoogleRichCards\\Hooks::onBeforePageDisplay",
		"ParserFirstCallInit": "MediaWiki\\Extension\\GoogleRichCards\\Hooks::onParserFirstCallInit"
	},
	"manifest_version": 2
}
